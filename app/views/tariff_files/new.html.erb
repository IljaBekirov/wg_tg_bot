<h1>Создать новый конфиг</h1>
<%= form_with model: @tariff_file, url: tariff_files_path, local: true do |form| %>
  <div class="form-group">
    <%= form.label :name, "Имя конфига" %>
    <%= form.text_field :name %>
  </div>

  <div class="form-group">
    <%= form.label :quantity, "Количество" %>
    <%= form.number_field :quantity %>
  </div>

  <div class="form-group">
    <%= form.submit "Создать" %>
  </div>
<% end %>
